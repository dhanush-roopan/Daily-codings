
#include <stdio.h>

int main()
{
    int a;
    scanf("%d",&a);
    for(int k =1;k<=a;k++){
    int n;
    scanf("%d",&n);
    if(n==0 || n==1){
        printf("no\n");
    }else{
    int flag = 1;
    for(int i =2;i<=n/2;i++){
        if(n%i==0){
            flag = 0;
            break;
            
        }
    }
    
    if(flag==1){
        printf("yes\n");
    }else{
        printf("no\n");
    }
    }
    }
}
